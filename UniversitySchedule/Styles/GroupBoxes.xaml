<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:local="clr-namespace:UniversitySchedule"
					xmlns:System="clr-namespace:System;assembly=mscorlib">
	<Style x:Key="MainGroupBox" TargetType="GroupBox">
		<Setter Property="FontFamily" Value="{StaticResource LatoRegular}"/>
		<Setter Property="FontSize" Value="{StaticResource FontSizeLarge}"/>
		<Setter Property="Foreground" Value="{StaticResource WordGreenBrush}"/>
		<Setter Property="BorderBrush" Value="{StaticResource MainBackGroundBrush}"/>
		<Setter Property="BorderThickness" Value="3"/>
	</Style>
	<Style x:Key="AverageTextGroupBox" TargetType="GroupBox">
		<Setter Property="FontFamily" Value="{StaticResource LatoRegular}"/>
		<Setter Property="FontSize" Value="{StaticResource FontSizeAverage}"/>
		<Setter Property="Foreground" Value="{StaticResource WordGreenBrush}"/>
		<Setter Property="BorderBrush" Value="{StaticResource  BackGroundVeryLightBrush}"/>
		<Setter Property="BorderThickness" Value="2"/>
	</Style>






	

</ResourceDictionary>